    "@typechain/ethers-v5": "7.0.0",
    "@typechain/hardhat": "2.0.1",
    "@types/chai": "4.2.18",
    "@types/mocha": "8.2.2",
    "dotenv": "^9.0.2",
    "ethereum-waffle": "3.3.0",
    "ethers": "^5.6.2",
    "hardhat": "2.17.1",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^6.0.0",
    "prettier": "^2.8.8",
